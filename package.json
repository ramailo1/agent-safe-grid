{
  "name": "agent-safe-grid",
  "version": "2.4.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 tsx -r dotenv/config backend/server.ts",
    "seed": "tsx backend/seeds/createInitialAdmin.ts",
    "restart-backend": "powershell -ExecutionPolicy Bypass -File ./scripts/restart-backend.ps1",
    "stop-backend": "powershell -ExecutionPolicy Bypass -File ./scripts/stop-backend.ps1",
    "test": "vitest run",
    "test:integration": "vitest run backend/tests/integration",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@google/genai": "^0.2.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.309.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.33",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^4.0.13"
  }
}
